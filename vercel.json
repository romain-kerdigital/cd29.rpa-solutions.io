{
  "buildCommand": "pip install -r requirements.txt && mkdocs build",
  "outputDirectory": "site",
  "installCommand": "pip install -r requirements.txt && npm ci",
  "framework": "mkdocs",
  "env": {
    "PYTHON_VERSION": "3.10"
  },
  "build": {
    "env": {
      "GOOGLE_ANALYTICS_KEY": "@google_analytics_key"
    }
  }
} 